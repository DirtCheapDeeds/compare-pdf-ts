{
  "name": "compare-pdf-ts",
  "version": "0.0.1",
  "author": "DirtCheapDeeds",
  "repository": "https://github.com/DirtCheapDeeds/compare-pdf-ts",
  "description": "In-memory PDF comparison tool designed for Node.js and TypeScript environments.",
  "license": "MIT",
  "keywords": [
    "pdf",
    "compare pdf"
  ],
  "files": [
    "package.json",
    "dist"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest --run",
    "build:types": "tsc",
    "bundle:cjs": "tsx build/bundle-cjs.ts",
    "bundle:esm": "tsx build/bundle-esm.ts",
    "build": "npm run build:types && npm run bundle:cjs && npm run bundle:esm",
    "quality-check": "npm install && npm run format && npm run lint:fix && npm run test && npm run build:types"
  },
  "devDependencies": {
    "@swc/core": "^1.15.8",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "ts-xor": "^1.3.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "pdfjs-dist": "^5.4.530",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0"
  }
}
